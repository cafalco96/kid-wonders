{
  "expo": {
    "name": "Wonder Kids",
    "slug": "wonder-kids",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wonder-kids.png",
    "scheme": "wonderkids",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wonder-kids.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cafalco.wonderkids"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-96x96.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wonder-kids.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e4daa731-4222-4826-a4e3-1b5163a5c412"
      }
    }
  }
}
